meta {
  name: Exception Updates (Subscription)
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/graphql/subscription/exception-updated
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "filters": {
      "interfaceTypes": ["ORDER_COLLECTION"],
      "severities": ["HIGH", "CRITICAL"],
      "includeResolved": false
    }
  }
}
