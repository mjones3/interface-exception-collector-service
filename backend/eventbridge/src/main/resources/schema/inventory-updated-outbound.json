{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "updateType": {
      "type": "string"
    },
    "unitNumber": {
      "type": "string"
    },
    "productCode": {
      "type": "string"
    },
    "productDescription": {
      "type": "string"
    },
    "productFamily": {
      "type": "string"
    },
    "bloodType": {
      "type": "string"
    },
    "expirationDate": {
      "type": "string"
    },
    "locationCode": {
      "type": "string"
    },
    "storageLocation": {
      "type": "string"
    },
    "inventoryStatus": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "properties": {
      "type": "object",
      "items": {
        "properties": {
          "key": {
            "type": "string",
            "minLength": 1
          },
          "value": {
            "type": "object",
            "minLength": 1
          }
        },
        "additionalProperties": false,
        "required": [
          "key",
          "value"
        ]
      }
    }
  },
  "required": [
    "updateType",
    "unitNumber",
    "productCode",
    "productDescription",
    "productFamily",
    "bloodType",
    "locationCode",
    "storageLocation",
    "inventoryStatus"
  ]
}
