            "bloodType": {
              "type": "string"
            },
            "quantity": {
              "type": "integer"
            },
            "comments": {
              "type": ["null","string"]
            }
          },
          "required": [
            "productFamily",
            "bloodType",
            "quantity"
          ]
        }
      ]
    }
  },
  "required": [
    "eventId",
    "occurredOn",
    "orderNumber",
    "externalId",
    "orderStatus",
    "locationCode",
    "createDate",
    "createEmployeeCode",
    "shipmentType",
    "priority",
    "shippingMethod",
    "productCategory",
    "shippingCustomerCode",
    "billingCustomerCode",
    "willPickUp",
    "orderItems"
