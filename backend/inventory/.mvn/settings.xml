<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 
                              http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>central</id>
      <username>${ARTIFACTORY_REGISTRY_USER}</username>
      <password>${ARTIFACTORY_REGISTRY_PASSWORD}</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>artifactory</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>https://artifactory.sha.ao.arc-one.com/artifactory/maven</url>
        </repository>
        <repository>
          <id>snapshots</id>
          <url>https://artifactory.sha.ao.arc-one.com/artifactory/maven</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>https://artifactory.sha.ao.arc-one.com/artifactory/maven</url>
        </pluginRepository>
        <pluginRepository>
          <id>snapshots</id>
          <url>https://artifactory.sha.ao.arc-one.com/artifactory/maven</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
</settings>