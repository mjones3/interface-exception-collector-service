            "cypressConfig": "apps/distribution-ui-e2e/cypress.json",
            "tsConfig": "apps/distribution-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "distribution-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "distribution-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/distribution-ui-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/distribution-ui-e2e/**/*"]
          }
        }
      }
