{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "touchable": {
      "projectType": "library",
      "root": "libs/touchable",
      "sourceRoot": "libs/touchable/src",
      "prefix": "rsa",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/@rsa/touchable"],
          "options": {
            "tsConfig": "libs/touchable/tsconfig.lib.json",
            "project": "libs/touchable/ng-package.json",
            "buildableProjectDepsInPackageJsonType": "dependencies"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/touchable/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/touchable/tsconfig.lib.json",
              "libs/touchable/tsconfig.spec.json",
              "libs/touchable/.storybook/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/touchable/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/touchable/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/touchable"]
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/touchable/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/touchable",
            "config": {
              "configFolder": "libs/touchable/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          },
          "outputs": ["{options.outputPath}"]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "touchable-e2e": {
      "root": "apps/touchable-e2e",
      "sourceRoot": "apps/touchable-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/touchable-e2e/cypress.json",
            "tsConfig": "apps/touchable-e2e/tsconfig.e2e.json",
            "devServerTarget": "touchable:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "touchable:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/touchable-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/touchable-e2e/**/*"]
          }
        }
      }
    },
