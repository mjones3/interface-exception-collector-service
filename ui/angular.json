    "distribution-ui": {
      "projectType": "application",
      "root": "apps/distribution-ui",
      "sourceRoot": "apps/distribution-ui/src",
      "prefix": "rsa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/distribution-ui",
            "index": "apps/distribution-ui/src/index.html",
            "main": "apps/distribution-ui/src/main.ts",
            "polyfills": "apps/distribution-ui/src/polyfills.ts",
            "tsConfig": "apps/distribution-ui/tsconfig.app.json",
            "aot": true,
            "allowedCommonJsDependencies": [
              "js-sha256",
              "hammerjs",
              "lodash",
              "highlight.js"
            ],
            "assets": [
              "apps/distribution-ui/src/favicon.ico",
              "apps/distribution-ui/src/assets",
              {
                "input": "libs/shared/assets/src",
                "glob": "**/*",
                "output": "assets"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/styles/src/lib"]
            },
            "styles": [
              "libs/shared/styles/src/lib/vendors.scss",
              "libs/shared/styles/src/lib/main.scss",
              "libs/shared/styles/src/lib/tailwind/distribution/tailwind.scss",
              "./node_modules/primeicons/primeicons.css",
              "./node_modules/primeng/resources/themes/md-light-indigo/theme.css",
              "./node_modules/primeng/resources/primeng.min.css",
              "apps/distribution-ui/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/distribution-ui/src/environments/environment.ts",
                  "with": "apps/distribution-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "1000kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "distribution-ui:build",
            "proxyConfig": "proxy.conf.json",
            "open": true,
            "port": 4200
          },
          "configurations": {
            "production": {
              "browserTarget": "distribution-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "distribution-ui:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/distribution-ui/tsconfig.app.json",
              "apps/distribution-ui/tsconfig.spec.json",
              "apps/distribution-ui/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/distribution-ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/distribution-ui"],
          "options": {
            "jestConfig": "apps/distribution-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "distribution-ui-e2e": {
      "root": "apps/distribution-ui-e2e",
      "sourceRoot": "apps/distribution-ui-e2e/src",
      "projectType": "application",
