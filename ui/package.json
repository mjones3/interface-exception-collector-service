{
  "name": "rsa",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve --open",
    "donor-ui-test": "nx run donor-ui:test --max-old-space-size=4096 --watch=false",
    "admin-ui-test": "nx run admin-ui:test --max-old-space-size=4096 --watch=false",
    "distribution-ui-test": "nx run distribution-ui:test --max-old-space-size=4096 --watch=false",
    "manufacturing-ui-test": "nx run manufacturing-ui:test --max-old-space-size=4096 --watch=false",
    "specialty-lab-ui-test": "nx run specialty-lab-ui:test --max-old-space-size=4096 --watch=false",
    "admin-ui": "nx run admin-ui:serve",
    "donor-ui": "nx run donor-ui:serve",
    "manufacturing-ui": "nx run manufacturing-ui:serve",
    "distribution-ui": "nx run distribution-ui:serve --open --proxy-config proxy.conf.json",
    "specialty-lab-ui": "nx run specialty-lab-ui:serve",
    "start:dev": "ng serve donor-ui --open --proxy-config proxy.conf.json",
    "start:dev:hmr": "ng serve manufacturing-ui --configuration hmr --hmr --liveReload false --proxy-config proxy.conf.json",
    "start:mem": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:prod:mem": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod",
    "test": "ng test",
    "test:coverage": "ng test --coverage --watch=false",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "tw": "npm run tw:build && npm run tw:export",
    "tw:build": "./node_modules/.bin/tailwind build src/tailwind/main.css -c src/tailwind/config.js -o src/styles/tailwind.scss",
    "tw:export": "npm run tw:export:js && npm run tw:export:scss",
    "tw:export:js": "node src/@treo/tailwind/export.js -c src/tailwind/config.js -o src/@treo/tailwind/exported/variables.ts",
    "tw:export:scss": "./node_modules/.bin/tailwind build src/@treo/tailwind/export.css -c src/tailwind/config.js -o src/@treo/tailwind/exported/_variables.scss",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "workspace-generator": "nx workspace-generator",
    "bundle-report": "nx run manufacturing-ui:build --prod --stats-json && webpack-bundle-analyzer dist/apps/manufacturing-ui/stats.json",
    "build-all": "npx cross-env NODE_OPTIONS=\"--trace-warnings\" nx run-many --target=build --all --prod --with-deps --parallel --maxParallel=7"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.3",
    "@angular/cdk": "11.0.3",
    "@angular/common": "11.0.3",
    "@angular/compiler": "11.0.3",
    "@angular/core": "11.0.3",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "11.0.3",
    "@angular/material": "11.0.3",
    "@angular/material-moment-adapter": "10.0.2",
    "@angular/platform-browser": "11.0.3",
    "@angular/platform-browser-dynamic": "11.0.3",
    "@angular/router": "11.0.3",
    "@ngrx/effects": "10.0.0",
    "@ngrx/entity": "10.0.0",
    "@ngrx/router-store": "10.0.0",
    "@ngrx/store": "10.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@nrwl/angular": "11.0.18",
    "angular-2-local-storage": "^3.0.2",
    "apexcharts": "3.19.3",
    "crypto-js": "3.3.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "^10.4.1",
    "jquery": "^3.5.1",
    "jwt-decode": "^4.0.0",
    "keycloak-js": "^19.0.3",
    "lodash-es": "4.17.15",
    "moment": "2.27.0",
    "ng-apexcharts": "^1.5.6",
    "ng2-pdf-viewer": "^7.0.2",
    "ngx-bootstrap": "^6.2.0",
    "ngx-cookie-service": "^10.1.1",
    "ngx-mask": "12.0.0",
    "ngx-material-file-input": "^2.1.1",
    "ngx-toastr": "^13.2.0",
    "perfect-scrollbar": "1.5.0",
    "primeicons": "^5.0.0",
    "primeng": "^11.4.5",
    "rxjs": "6.5.5",
    "simple-keyboard": "^2.29.67",
    "tslib": "2.0.0",
    "uuid": "^9.0.0",
    "web-animations-js": "2.3.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular-devkit/core": "^11.0.7",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.3",
    "@angular/language-service": "11.0.3",
    "@angularclass/hmr": "^3.0.0",
    "@commitlint/cli": "^11.0.0",
    "@ngrx/schematics": "10.0.0",
    "@ngrx/store-devtools": "10.0.0",
    "@nrwl/cli": "11.0.18",
    "@nrwl/cypress": "11.0.18",
    "@nrwl/jest": "11.0.18",
    "@nrwl/storybook": "11.0.18",
    "@nrwl/tao": "11.0.18",
    "@nrwl/workspace": "11.0.18",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/angular": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.2",
    "@types/angular-material": "^1.1.71",
    "@types/crypto-js": "3.1.47",
    "@types/highlight.js": "9.12.4",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "26.0.8",
    "@types/lodash": "4.14.167",
    "@types/node": "12.12.47",
    "barrelsby": "^2.2.0",
    "codelyzer": "6.0.0-next.1",
    "comment-json": "^4.1.0",
    "commitizen": "^4.2.2",
    "commitlint-config-cz": "^0.13.2",
    "conventional-changelog-cli": "2.1.1",
    "cypress": "^4.1.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.2.5",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "jest": "26.2.2",
    "jest-canvas-mock": "^2.2.0",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "^10.2.11",
    "lodash": "4.17.21",
    "ng-packagr": "^10.0.0",
    "prettier": "2.1.2",
    "protractor": "7.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "tailwindcss": "^1.4.6",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "4.0.5",
    "typescript-tslint-plugin": "^0.5.5",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --concurrent false",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "docs/**/*.md": [
      "node docs/check-kb-labels",
      "node docs/check-dead-links fast",
      "node docs/check-documentation-overview",
      "node docs/check-sentence-newline"
    ],
    "*.{scss,css}": [
      "stylelint --fix"
    ],
    "*": [
      "prettier --loglevel warn --write"
    ],
    "*.ts": [
      "tslint --project tsconfig.base.json --fix",
      "jest --ci --bail --findRelatedTests --detectOpenHandles"
    ]
  },
  "commitlint": {
    "extends": [
      "cz"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "postinstall": {
    "jest-extended/types": "copy node_modules/@types/jest-extended"
  }
}
